{
  "name": "boilerplate-ts",
  "version": "1.0.0",
  "description": "typescript starter",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint ./src --ext .ts"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.27.0",
    "eslint-config-react-typescript": "^1.0.7",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-alias": "^1.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2"
  },
  "dependencies": {},
  "peerDependencies": {},
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "alias"
    }
  },
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ruanyl/boilerplate-ts.git"
  },
  "homepage": "https://github.com/ruanyl/boilerplate-ts",
  "bugs": "https://github.com/ruanyl/boilerplate-ts/issues"
}
